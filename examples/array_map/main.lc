
function main() {
    const arr = [ "1", "2", "3", "4", "5", "6", "7", "8" ];
    let counter = 0;
    const tuples = arr.map((item: string): (string, i32) => {
        const tmp = counter;
        counter += 1;
        (item, tmp)
    });

    let index = 0;
    while index < tuples.length {
        const item = tuples[index];
        match item {
            case (name, index) => print("name:", name, "index:", index)
        }
        index += 1;
    }
}