
function generate(): () => Option<string> {
    const data = [
        "Amy",
        "Bobby",
        "Candy",
        "Daddy",
        "Fancy",
    ];
    let index = 0;
    return (): Option<string> => {
        if index >= data.length {
            return None;
        }
        const result = data[index];

        index += 1;

        Some(result)
    };
}

function isSome(o: Option<string>): boolean {
    match o {
        case Some(_) => true
        case None => false
    }
}

function main() {
    const generateor = generate();
    let index = 0;
    while isSome(generateor()) {
        const content = "ok";
        print(content, index);

        if index == 3 {
            break;
        }

        index += 1;
    }
}
