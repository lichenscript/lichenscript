
function main() {
    const a = [1,2,3,4,5,6,7,8];
    print("arr len:", a.length);

    let i = 0;
    while i < 100 {
        a.push(i);
        i += 1;
    }

    print("arr len after push:", a.length);

    a[0] = 3;
    print("a[0]:", a[0]);

    a.resize(0, 0);
    print("length:", a.length);

    a.resize(10, 6);
    print("length:", a.length);

    for item in a {
        print("item:", a[i]);
    }
}
